          "type": "string"
        },
        "instanceUrl": {
          "type": "string"
        },
        "clientId": {
          "type": "string"
        },
        "alias": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "connectedStatus": {
          "type": "string"
        },
        "sfdxAuthUrl": {
          "type": "string"
        },
        "createdBy": {
          "type": "string"
        },
        "createdDate": {
          "type": "string"
        },
        "expirationDate": {
          "type": "string"
        },
        "orgName": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "devHubId": {
          "type": "string"
        },
        "edition": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        },
        "snapshot": {
          "type": "string"
        },
        "lastUsed": {
          "type": "string",
